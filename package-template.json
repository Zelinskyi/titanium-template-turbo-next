{
	"name": "${name}",
	"version": "1.0.0-0",
	"description": "${description}",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"prebuild": "ti sdk install",	
		"build": "npm run build:ios && npm run build:android",
		"build:ios": "ti build -p ios --build-only --log-level trace --ios-version 12.2",
		"build:android": "ti build -p android --build-only --log-level trace"
	},
	"license": "${license}",
	"dependencies": {},
	"devDependencies": {
		"lodash": ">=4.17.12",
		"@titanium/eslint-config": "^0.0.9",
		"@titanium/plugin-turbo": "^1.0.4",
		"@titanium/turbo": "latest",
		"eslint": "^6.2.1",
		"titanium": "^5.2.1"		
	},
	"author": {
		"name": "${author_name}",
		"email": "${author_email}",
		"url": "${author_url}"
	},
	"repository": {
		"type": "${repo_type}",
		"url": "${repo_url}"
	},
	"bugs": {
		"url": "${bugs_url}",
		"email": "${bugs_email}"
	},
	"keywords": ["titanium", "mobile", "alloy", "turbo", "appcelerator", "axway", "amplify"]
}
